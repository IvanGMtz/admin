<!DOCTYPE html
{% block avanzu_html_start %}{% endblock %}>
<html lang="es">
  <head>
      <title>
          {% block avanzu_document_title %}{{ block('avanzu_page_title') }}{% endblock %}
      </title>
      {% block avanzu_head %}{% endblock %}
      {% include('AvanzuAdminThemeBundle:Partials:_head_default.html.twig') %}
<style>
  .navbar .navbar-brand {
    font-weight: 600;
    margin: 5px 0px;
    padding: 0 0 0 20px;
    font-size: 20px;
  }
  .layout-navbar-fixed .wrapper .content-wrapper {
    margin-top: calc(6.6rem);
  }
</style>
  </head>
  <body{% block avanzu_body_start %}{% endblock %} class="layout-top-nav landing-page">
    {% block avanzu_after_body_start %}{% endblock %}
{#    <div class="container-fluid">#}
      <div class="wrapper">
      
      <nav class="main-header navbar navbar-expand flex-column flex-md-row bd-navbar" id="main-header">
        <a href="/" class="navbar-brand mr-0 mr-md-2">
          <img src="/bundles/appbundle/images/favicon.png" alt="GOlD Trade Company">
        </a>
      </nav>
      
      <div class="content-wrapper">
          {% block avanzu_page_content_before %}{% endblock %}
          {% block avanzu_page_content %}{% endblock %}
          {% block avanzu_page_content_after %}{% endblock %}
      </div>
          
      {% block avanzu_footer %}
      <footer class="footer">
          <div class="container">
              <div class="copyright">
                  &copy; 2021 <a href="#!">SISTEMASW3</a>, derechos reservados
              </div>
          </div>
      </footer>
      {% endblock %}
      </div>
{#    </div>#}
    {#<div class="floating-icon">
      <a href="https://www.forblink.com/index.php?phone=+573157197576&text=Hola!%20Te%20escribo%20desde%20la%20pagina%20inicial%20de%20UNLIMITEDBIT2%20-%20" target="_blank">
        <i class="fab fa-whatsapp text-green fa-3x "></i>
      </a>
    </div>#}
    {% include 'AvanzuAdminThemeBundle:Partials:_scripts.html.twig' %}
    {% block avanzu_javascripts %}{% endblock %}
    <script src="/bundles/landing/js/awesome-landing-page.js" type="text/javascript"></script>
    <script src="/bundles/landing/js/smooth-scroll.polyfills.min.js" type="text/javascript"></script>
    {% block avanzu_javascripts_inline %}{% endblock %}
    <script>
      $(function(){
        var scroll = new SmoothScroll('a[href*="#"]',{
          offset: 105
        });
      });
    </script>
    {% for label, messages in app.flashes %}
      {% for message in messages %}
        <script>
          Swal.fire({
            icon: '{{label}}',
            title: '{{message}}',
       {#     timer: 4000,
            timerProgressBar: true#}
          });
        </script>
      {% endfor %}
    {% endfor %}
  </body>
</html>
